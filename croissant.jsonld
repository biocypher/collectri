    "recordSet": [
      {
        "@type": "cr:RecordSet",
        "@id": "collectri",
        "name": "CollecTRI",
        "description": "Transcription factor → target gene regulatory interactions with supporting evidence.",
        "field": [
          {
            "@type": "cr:Field",
            "@id": "collectri/source",
            "name": "source",
            "description": "Gene symbol of transcription factor (TF).",
            "dataType": "sc:Text",
            "examples": [
              "MYC"
            ],
            "source": {
              "fileObject": {
                "@id": "collectri.csv"
              },
              "extract": {
                "column": "source"
              }
            }
          },
          {
            "@type": "cr:Field",
            "@id": "collectri/target",
            "name": "target",
            "description": "Gene symbol of the regulated target gene.",
            "dataType": "sc:Text",
            "examples": [
              "TERT"
            ],
            "source": {
              "fileObject": {
                "@id": "collectri.csv"
              },
              "extract": {
                "column": "target"
              }
            }
          },
          {
            "@type": "cr:Field",
            "@id": "collectri/weight",
            "name": "weight",
            "description": "Binary indicator of confidence (1 = supported interaction).",
            "dataType": "sc:Integer",
            "examples": [
              1
            ],
            "source": {
              "fileObject": {
                "@id": "collectri.csv"
              },
              "extract": {
                "column": "weight"
              }
            }
          },
          {
            "@type": "cr:Field",
            "@id": "collectri/tf_category",
            "name": "TF.category",
            "description": "Category of transcription factor.",
            "dataType": "sc:Text",
            "examples": [
              "dbTF"
            ],
            "source": {
              "fileObject": {
                "@id": "collectri.csv"
              },
              "extract": {
                "column": "TF.category"
              }
            }
          },
          {
            "@type": "cr:Field",
            "@id": "collectri/resources",
            "name": "resources",
            "description": "List of supporting data sources (semicolon-separated).",
            "dataType": "sc:Text",
            "examples": [
              "ExTRI;HTRI;TRRUST"
            ],
            "source": {
              "fileObject": {
                "@id": "collectri.csv"
              },
              "extract": {
                "column": "resources"
              }
            }
          },
          {
            "@type": "cr:Field",
            "@id": "collectri/pmid",
            "name": "PMID",
            "description": "PubMed references (semicolon-separated).",
            "dataType": "sc:Text",
            "examples": [
              "10022128;10491298"
            ],
            "source": {
              "fileObject": {
                "@id": "collectri.csv"
              },
              "extract": {
                "column": "PMID"
              }
            }
          },
          {
            "@type": "cr:Field",
            "@id": "collectri/sign_decision",
            "name": "sign.decision",
            "description": "Determination method for activation/inhibition sign.",
            "dataType": "sc:Text",
            "examples": [
              "PMID"
            ],
            "source": {
              "fileObject": {
                "@id": "collectri.csv"
              },
              "extract": {
                "column": "sign.decision"
              }
            }
          }
        ]
